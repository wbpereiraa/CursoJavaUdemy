public class ExercicioEstruturaDeControle7 {

	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);

		String numero = "";
		
		while(!numero.equalsIgnoreCase("-")) {
			numero = entrada.next();
			System.out.println("Digite um n√∫mero: ");
		}

		entrada.close();
	}
}
