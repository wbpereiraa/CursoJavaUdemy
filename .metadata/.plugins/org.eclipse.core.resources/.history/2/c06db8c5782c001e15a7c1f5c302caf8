package oo.composicao.desafio;



public class Produto {

	double preco;
	String nome;
	
	public Produto(double preco, String nome) {
		super();
		this.preco = preco;
		this.nome = nome;
	}
	
	
	public double getPreco() {
		return preco;
	}
	public void setPreco(double preco) {
		this.preco = preco;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
}
